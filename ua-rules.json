{
 "name": "AD-Capital",
 "comments": "Agents for AD-Capital",
 "config": {},
 "rules": [
   {
     "name": "java-4.3.3.0",
     "monitor": "java",
     "comments": "Update Java Agent to 4.3.3.0",
     "config":
     {
       "version": "4.3.3.0",
       "state": "attached",
       "application_name": "$javaenv_APP_NAME",
       "tier_name": "$javaenv_TIER_NAME",
       "node_name": "$javaenv_NODE_NAME",
       "deploy_cmd": ".*jmxremote.*",
       "additional_props": "appdynamics.analytics.agent.url=http://localhost:9090/v2/sinks/bt"
     },
     "condition": "True"
   },
   {
     "name": "universal-4.3.3.0",
     "monitor": "universal",
     "comments": "Update Universal Agent to 4.3.3.0",
     "config": {
       "state": "started",
       "version": "4.3.3.0"
     },
     "condition": "True"
   }
 ]
}
